<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @photo.name %>
</p>

<p>
  <strong>Image:</strong>
  <%= image_tag @photo.image.thumb.url %> 
</p>

<% if user_signed_in? %>
  <% if current_user.manageAlbum? %>
    <%= link_to 'Edit', edit_photo_path(@photo) %> |
  <% end %>
<% end %>
<%= link_to 'Back', photos_path %>